<p id="notice"><%= notice %></p>

<h1>Walkings</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Volunteer</th>
      <th>Animal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @walkings.each do |walking| %>
      <tr>
        <td><%= walking.date %></td>
        <td><%= walking.volunteer.name %></td>
        <td><%= walking.animal.name %></td>
        <td><%= link_to 'Show', walking %></td>
        <td><%= link_to 'Edit', edit_walking_path(walking) %></td>
        <td><%= link_to 'Destroy', walking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

