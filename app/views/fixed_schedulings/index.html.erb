<p id="notice"><%= notice %></p>

<h1>Fixed Schedulings</h1>

<table>
  <thead>
    <tr>
      <th>Day of week</th>
      <th>Volunteer</th>
      <th>Animal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fixed_schedulings.each do |fixed_scheduling| %>
      <tr>
        <td><%= fixed_scheduling.day_of_week %></td>
        <td><%= fixed_scheduling.volunteer %></td>
        <td><%= fixed_scheduling.animal %></td>
        <td><%= link_to 'Show', fixed_scheduling %></td>
        <td><%= link_to 'Edit', edit_fixed_scheduling_path(fixed_scheduling) %></td>
        <td><%= link_to 'Destroy', fixed_scheduling, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

